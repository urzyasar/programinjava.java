import java.util.Scanner;


public class NtimesHello {

    public static void main(String[] args) {
        int i,n;
        Scanner sc=new Scanner(System.in);
        n=sc.nextInt();
        for (i=0;i<=n;i++)
        {
            System.out.println("Hello");
        }
          
      
    }
    
}
